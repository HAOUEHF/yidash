{
    "name": "yidash",
    "type": "module",
    "version": "1.6.0",
    "private": false,
    "license": "MIT",
    "description": "yidash（易大师）一万个JavaScript实用方法库",
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    },
    "author": "chensuiyi <bimostyle@qq.com>",
    "homepage": "https://yidash.dev",
    "repository": {
        "type": "git",
        "url": "https://github.com/chenbimo/yidash"
    },
    "keywords": [
        "lodash",
        "utils",
        "helper",
        "help"
    ],
    "exports": {
        ".": "./lib.js",
        "./node": "./libNode.js",
        "./browser": "./libBrowser.js",
        "./libNames.js": "./libNames.js",
        "./libNodeNames.js": "./libNodeNames.js",
        "./libBrowserNames.js": "./libBrowserNames.js"
    },
    "files": [
        "config/",
        "lib/",
        "index.js",
        "LICENSE",
        "package.json",
        "README.md",
        "lib.js",
        "libNames.js",
        "libNode.js",
        "libNodeNames.js",
        "libBrowser.js",
        "libBrowserNames.js"
    ],
    "scripts": {
        "gen": "node generate.js",
        "docs": "del-cli docs & jsdoc -c jsdoc.json",
        "test": "vitest",
        "test-ui": "vitest --ui",
        "lint": "eslint ./lib",
        "lint:fix": "eslint ./lib --fix",
        "format": "prettier --write .",
        "inspect": "eslint --inspect-config"
    },
    "dependencies": {
        "date-fns": "^3.6.0",
        "nanoid": "^5.0.7"
    },
    "devDependencies": {
        "@eslint/config-inspector": "^0.5.4",
        "@eslint/js": "^9.9.1",
        "@vitest/ui": "^2.0.5",
        "colorette": "^2.0.20",
        "del-cli": "^5.1.0",
        "docdash-extended-chensuiyi": "^1.1.1",
        "eslint": "^9.9.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-jsdoc": "^50.2.2",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-regexp": "^2.6.0",
        "fs-extra": "^11.2.0",
        "globals": "^15.9.0",
        "happy-dom": "^15.0.0",
        "jsdoc": "^4.0.3",
        "lint-staged": "^15.2.9",
        "log-symbols": "^7.0.0",
        "prettier": "^3.3.3",
        "simple-git-hooks": "^2.11.1",
        "vitest": "^2.0.5"
    },
    "simple-git-hooks": {
        "pre-commit": "pnpm exec lint-staged"
    },
    "lint-staged": {
        "*": [
            "prettier --write --cache --ignore-unknown"
        ]
    }
}
